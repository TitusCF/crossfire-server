install-dist_admSCRIPTS: $(dist_adm_SCRIPTS)
	@$(NORMAL_INSTALL)
	test -z "$(admdir)" || $(mkdir_p) "$(DESTDIR)$(admdir)"
	@list='$(dist_adm_SCRIPTS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(dist_admSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(admdir)/$$f'"; \
	    $(dist_admSCRIPT_INSTALL) "$$d$$p" "$(DESTDIR)$(admdir)/$$f"; \
	  else :; fi; \
	done

uninstall-dist_admSCRIPTS:
	@$(NORMAL_UNINSTALL)
	@list='$(dist_adm_SCRIPTS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	  echo " rm -f '$(DESTDIR)$(admdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(admdir)/$$f"; \
	done


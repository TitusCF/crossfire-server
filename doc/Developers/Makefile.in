VPATH = @srcdir@
srcdir = @srcdir@

RM=@RM@
BASENAME = @BASENAME@

PFILES = \
	Makefile.in  README  cvs    images       map-technical  mapguide \
	objects      plugins        programming_guide  protocol       python \
	random-maps  random-styles runes skills teleporters testplans


FILES = $(PFILES)

prefix = @prefix@
exec_prefix = @exec_prefix@

MKDIR = @MKDIR@
SHELL = @BOURNE_SHELL@

all:

depend::

clean::

distclean:: clean
	$(RM) -f Makefile


archive:
	@if [ ! -d $(ARCHIVE)/doc/Developers ]; then $(MKDIR) $(ARCHIVE)/doc/Developers; fi
	$(CP) $(FILES) $(ARCHIVE)/doc/Developers

patchlist::
	@echo $(PFILES) > .patchlist


docarchive::


install::
